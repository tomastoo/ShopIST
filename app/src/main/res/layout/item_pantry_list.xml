    android:paddingVertical="15dp">
    <TextView
        android:id="@+id/txt_view_name_pantry_item"
        android:lines="1"
        android:layout_width="330dp"
        android:layout_height="wrap_content"
        android:text="Latas de atum 150fff fddddeeeeedd"
        android:textSize="25dp"
        />
    <TextView
        android:id="@+id/txt_view_stock_quantity"
